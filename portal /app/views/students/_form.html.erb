<%= form_for(student) do |f| %>
  <% if student.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(student.errors.count, "error") %> prohibited this student from being saved:</h2>

      <ul>
      <% student.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :rollno %>
    <%= f.number_field :rollno %>
  </div>

  <div class="field">
    <%= f.label :standard %>
    <%= f.text_field :standard %>
  </div>

  <div class = "well">
   
      
      <%= f.label :avatar %>
      <%= f.file_field :avatar %>  
  </div>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
